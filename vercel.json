{
  "rewrites": [
    {
      "source": "/blog/sitemap.xml",
      "destination": "https://blog.smartstockalert.io/blog/sitemap.xml"
    },
    {
      "source": "/blog/sitemap.xsl",
      "destination": "https://blog.smartstockalert.io/blog/sitemap.xsl"
    },
    {
      "source": "/blog/:path*/",
      "destination": "https://blog.smartstockalert.io/blog/:path*/"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog.smartstockalert.io/blog/:path*"
    }
  ],
  "headers": [
    {
      "source": "/blog/:path*",
      "headers": [{ "key": "x-forwarded-host", "value": "smartstockalert.io" }]
    }
  ]
}
