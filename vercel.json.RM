{
  "rewrites": [
    {
      "source": "/blog/:path*/",
      "destination": "https://blog.smartstockalert.io/blog/:path*/"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog.smartstockalert.io/blog/:path*"
    }
  ],
  "headers": [
    {
      "source": "/blog/:path*",
      "headers": [{ "key": "x-forwarded-host", "value": "smartstockalert.io" }]
    }
  ]
}
